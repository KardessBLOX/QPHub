    game:GetService("StarterGui"):SetCore("SendNotification",{
    Title = "â”ƒ SCRIPT By QP â”ƒ",
    Text = "ToggleBlockSpam",
    Duration = 50
})

getgenv().SpamSpeed = 1 -- 1-25


local DrRayLibrary = loadstring(game:HttpGet("https://raw.githubusercontent.com/AZYsGithub/DrRay-UI-Library/main/DrRay.lua"))()

local window = DrRayLibrary:Load("QP Hub", "Default")

local Bladeball = DrRayLibrary.newTab("Bladeball", "ImageIdHere")


Bladeball.newLabel("Made by @ScriptsQP :)")
Bladeball.newButton("AutoParryV1", "basic :) And Runs Smoothly", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/Hosvile/Refinement/main/MC%3ABlade%20Ball%20Parry",true))()
end)

